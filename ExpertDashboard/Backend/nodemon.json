{
  "verbose": false,
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "*.test.js"
  ],
  "watch": [
    "*.js",
    "*.json"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json",
  "delay": 1000,
  "signal": "SIGTERM",
  "execMap": {
    "js": "node"
  }
}
